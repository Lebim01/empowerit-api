{
  "version": 2,
  "functions": {
    "**/*": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ]
}
